{
    "name": "phxlang/phx",
    "description": "AST-based syntax extension for PHP",
    "license": "MIT",
    "require": {
        "php": ">=7.0.0",
        "nikic/php-parser": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.2"
    },
    "bin": [
        "bin/phx"
    ],
    "autoload": {
        "psr-4": {
            "Phx\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Phx\\Tests\\": "tests/"
        }
    }
}